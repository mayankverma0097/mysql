CREATE TABLE teachers (
    teacher_id INT PRIMARY KEY,
    teacher_name VARCHAR(100) NOT NULL,
    course_id INT
);
INSERT INTO teachers (teacher_id, teacher_name, course_id) VALUES
(1, 'Dr. Sharma', 1),   
(2, 'Prof. Verma', 2),  
(3, 'Dr. Khan', 6),     
(4, 'Ms. Mehta', 3),    
(5, 'Mr. Gupta', 10);   




mysql> select s.first_name,s.last_name,c.course_name,c.fees,t.teacher_name from studentDetails s left join courses c on s.course_id = c.course_id left join teachers t on s.course_id = t.course_id;
