mysql> select first_name,last_name,fees_paid,
    -> case
    -> when fees_paid >= 20000 then "fees done"
    -> when fees_paid >= 15000 then "2nd installment complete"
    -> when fees_paid >= 10000 then "1st installment done"
    -> else "addmission incomplete"
    -> end as fees_status from studentDetails;
